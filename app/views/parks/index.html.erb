<h1> Listing Parks </h1>


<%= will_paginate %>
<table> 
		<tr>
			<th> ID: </th>
			<th> Name </th> 
			<th> City </th>
			<th> State </th>
			<th> Country </th>
			<th> Latitude </th>
			<th> Longitude </th>
			<th></th>
			<th></th>
			<th></th>
			<th></th>
			<th></th>
			<th></th>
			<th></th>
			<th></th>
		</tr> 

	<% @parks.each do |park| %>
		<tr>
			<td><%= park.id %></td>
			<td><%= park.name %></td>
			<td><%= park.city %></td>
			<td><%= park.state %></td>
			<td><%= park.country %></td>
			<td><%= park.latitude %></td>
			<td><%= park.longitude %></td>
			<td><%= link_to 'Show', park %></td>
			<td><%= link_to 'Edit', edit_park_path(park) %> </td>
			<td><%= link_to 'Destroy', park_path(park), method: :delete, data: {confirm: 'Are you sure?'} %></td>
		</tr>
	<% end %>
</table>
<%= will_paginate %>

